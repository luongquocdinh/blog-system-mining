var osmosis = require('osmosis')

var url = 'http://www.bongda.com.vn/tin-moi-nhat/'
osmosis
.get(url)
.set({
    "title": "h2 > a",
    "url": "h1 > a @href",
    "brief": ".sapo_news"
})
.data(function (data) {
   // get data of all value in set
   console.log(data)
})
.log(console.log)
.error(console.log)
.debug(console.log)
